name: "SHnet"
layer {
name: "data"
type: "Input"
top: "data"
input_param {
shape: { dim: 128 dim: 32}
}
}
layer {
name: "label"
type: "Input"
top: "label"
input_param {
shape: { dim: 128 dim: 48}
}
}

#slice to 16 pts
layer {
  name: "slice_layer"
  type: "Slice"
  bottom: "data"
  top: "pts1"
  top: "pts2"
  top: "pts3"
  top: "pts4"
  top: "pts5"
  top: "pts6"
  top: "pts7"
  top: "pts8"
  top: "pts9"
  top: "pts10"
  top: "pts11"
  top: "pts12"
  top: "pts13"
  top: "pts14"
  top: "pts15"
  top: "pts16"
  slice_param {
    axis: 1
    slice_point: 2
    slice_point: 4
    slice_point: 6
    slice_point: 8
    slice_point: 10
    slice_point: 12
    slice_point: 14
    slice_point: 16
    slice_point: 18
    slice_point: 20
    slice_point: 22
    slice_point: 24
    slice_point: 26
    slice_point: 28
    slice_point: 30
  }
}


